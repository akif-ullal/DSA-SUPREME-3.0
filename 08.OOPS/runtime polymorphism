#include <iostream>
using namespace std;
class shape {
  public:
   virtual void print() 
  {
    cout<<"generic shape.."<<endl;
  }
};

class circle : public shape
{
  public:
  void print()
  {
    cout<<"circle shape"<<endl;
  }
};

void draw(shape*s)
{
  s->print();
}

int main() {
  shape A;
  circle B;

  draw(&B);
  return 0;
}
