#include <iostream>
using namespace std;

class Add{
  public:
  int sum(int a,int b)
  {
    return a+b;
  }

   int sum(int a,int b,int c)
  {
    return a+b+c;
  }

  double sum(double a,double b)
  {
    return a+b;
  }
};

int main() {
  int a=4;
  int b=5;
  int c=6;
  Add A;

  cout<<A.sum(a,b)<<endl;
  cout<<A.sum(5.5,5.5)<<endl;
  cout<<A.sum(a,b,c)<<endl;
  return 0;
}
