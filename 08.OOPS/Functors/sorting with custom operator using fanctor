#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;

class cmp{
   public: 
   cmp()
   {

   }
   bool operator()(int a,int b)
   {
      return a<b;
   }
};

int main()
{
   vector<int>arr={50,12,100};

   cmp c;

   sort(arr.begin(),arr.end(),c);

   for(int i:arr)
   {
      cout<<i<<" ";
   }
   cout<<endl;


}
