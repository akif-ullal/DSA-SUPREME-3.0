//simple class
#include<iostream>
using namespace std;
class abc{
    public:
    int x=0;
    int y=0;

    public:
    void print()
    {
        cout<<x<<" "<<y<<endl;
    }
};
int main()
{
    abc obj1={1,2};
    abc obj2={3,4};

    obj1.print();
    obj2.print();
}



//static data 
#include<iostream>
using namespace std;
class abc{
    public:
    static int x;
    static int y;

    public:
    void print()
    {
        cout<<x<<" "<<y<<endl;
    }
};
int abc::x;
int abc::y;
int main()
{
   abc obj1;
   obj1.x=1;
   obj1.y=2;

   obj1.print();

   abc obj2;

   obj2.x=3;
   obj2.y=4;

   obj1.print();
   obj2.print();


}


