#include <iostream>
#include<stack>
using namespace std;

int main() {
  stack<int>s;
  s.push(10);
  s.push(20);
  s.push(30);

  cout<<s.size()<<endl;

  cout<<s.empty()<<endl;

  cout<<s.top()<<endl;

  while(!s.empty())
  {
    cout<<s.top()<<" ";
    s.pop();
  }
  cout<<endl;

  cout<<s.empty()<<endl;
  return 0;
}
