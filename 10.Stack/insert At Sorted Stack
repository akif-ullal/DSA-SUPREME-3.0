#include <iostream>
#include<stack>
#include <limits.h>
using namespace std;
void insertAtSortedStack(stack<int>&s,int value)
{
  if(!s.empty() && s.top() < value)
  {
    s.push(value);
    return;
  }

  int topele=s.top();
  s.pop();
  insertAtSortedStack(s,value);
  s.push(topele);
}
void print(stack<int>&s)
{
  while(!s.empty())
  {
    cout<<s.top()<<" ";
    s.pop();
  }
  cout<<endl;
}
int main() {
  stack<int>s;
  s.push(10);
  s.push(20);
  s.push(30);
  s.push(40);
  s.push(50);

  int value=23;
  insertAtSortedStack(s,value);
  print(s);
  
  return 0;
}
  
